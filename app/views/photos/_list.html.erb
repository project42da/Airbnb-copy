<% if @photos%>
	<div class="row">
		<%@photos.each do |photo|%>
			<div class="col-md-4">
				<div class="panel panel-pefault">
					<div class="panel-heading preview">
						<%=image_tag photo.image.url()%>
					</div>
					<div class="panel-body">
						<span class="pull-right">
							<i class="fa fa-times fa-lg"></i>
						</span>
					</div>
				</div>
			</div>
		<%end%>
	</div>
<%end%>